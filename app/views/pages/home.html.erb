    <!-- CONTACT -->
    <section class='white trans' id='contact'>
      <div class='centered large'>
        <div class='row-fluid'>
          <div class='col-md-4'>
            <h2>
              Contact
            </h2>
            <p class='small'>
              Suspendisse ultricies elit id consectetur pharetra. Aenean accumsan venenatis massa id mollis. Donec quis pharetra lacus. Aenean bibendum, nunc eget mollis blandit.
            </p>
            <div class='social-icons'>
              <a class='icon fa fa-twitter' href='#'></a>
              <a class='icon fa fa-facebook' href='#'></a>
              <a class='icon fa fa-dribbble' href='#'></a>
              <a class='icon fa fa-instagram' href='#'></a>
            </div>
          </div>
          <div class='col-md-8'>
            <h3>
              Get in touch
            </h3>
             <form action='https://formspree.io/javoor1@hotmail.com' id='form' name='form' method="POST">
              <div class='form-group'>
                <input class='form-control' id='name' name='name' placeholder='Name' type='text'>
                <span class='glyphicon glyphicon-remove form-control-feedback'></span>
              </div>
              <div class='form-group'>
                <input class='form-control' id='email' name='email' placeholder='E-mail' type='text'>
                <span class='glyphicon glyphicon-remove form-control-feedback'></span>
              </div>
              <div class='form-group'>
                <input class='form-control' id='phone' name='phone' placeholder='Phone' type='text'>
              </div>
              <div class='form-group'>
                <textarea class='form-control' id='message' name='message' placeholder='Message'></textarea>
                <span class='glyphicon glyphicon-remove form-control-feedback'></span>
              </div>
              <input class='form-control' id='send' type='submit' value='Send'>
            </form>
          </div>
        </div>
      </div>
    </section>
